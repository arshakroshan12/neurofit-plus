{
  "openapi": "3.1.0",
  "info": {
    "title": "NeuroFit+ Test App",
    "version": "0.1.0"
  },
  "paths": {
    "/": {
      "get": {
        "summary": "Root",
        "operationId": "root__get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/health": {
      "get": {
        "summary": "Health",
        "operationId": "health_health_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/model/features": {
      "get": {
        "summary": "Model Features",
        "operationId": "model_features_model_features_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/predict_fatigue": {
      "post": {
        "summary": "Predict Fatigue",
        "operationId": "predict_fatigue_predict_fatigue_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PredictRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AnswerModel": {
        "properties": {
          "question_id": {
            "type": "string",
            "title": "Question Id"
          },
          "value": {
            "type": "number",
            "title": "Value"
          }
        },
        "type": "object",
        "required": [
          "question_id",
          "value"
        ],
        "title": "AnswerModel"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "PredictRequest": {
        "properties": {
          "timestamp": {
            "type": "string",
            "title": "Timestamp"
          },
          "answers": {
            "anyOf": [
              {
                "items": {
                  "$ref": "#/components/schemas/AnswerModel"
                },
                "type": "array"
              },
              {
                "additionalProperties": {
                  "type": "number"
                },
                "type": "object"
              }
            ],
            "title": "Answers"
          },
          "typing_features": {
            "$ref": "#/components/schemas/TypingModel"
          },
          "task_performance": {
            "$ref": "#/components/schemas/TaskPerfModel"
          }
        },
        "type": "object",
        "required": [
          "timestamp",
          "answers",
          "typing_features",
          "task_performance"
        ],
        "title": "PredictRequest"
      },
      "TaskPerfModel": {
        "properties": {
          "reaction_time_ms": {
            "type": "number",
            "title": "Reaction Time Ms"
          },
          "reaction_attempted": {
            "type": "boolean",
            "title": "Reaction Attempted",
            "default": false
          }
        },
        "type": "object",
        "title": "TaskPerfModel"
      },
      "TypingModel": {
        "properties": {
          "average_latency_ms": {
            "type": "number",
            "title": "Average Latency Ms",
            "default": 0.0
          },
          "total_duration_ms": {
            "type": "number",
            "title": "Total Duration Ms",
            "default": 0.0
          },
          "backspace_rate": {
            "type": "number",
            "title": "Backspace Rate",
            "default": 0.0
          }
        },
        "type": "object",
        "title": "TypingModel"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
    }
  }
}